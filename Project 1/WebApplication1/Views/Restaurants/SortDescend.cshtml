@model IEnumerable<ClassLibrary1.RestaurantsClass>

@{
    ViewBag.Title = "SortDescend";
}

<h2>Restaurants in Descending Order</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Restaurant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rating)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cuisine)
        </th>
        

    
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Restaurant)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rating)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cuisine)
        </td>

      
    </tr>
    }

</table>

<footer> @Html.ActionLink("Back to List", "Index")</footer>

